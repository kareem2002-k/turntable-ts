# Database URL for Prisma with Supabase
DATABASE_URL="postgresql://postgres:[YOUR-PASSWORD]@[YOUR-PROJECT-REF].supabase.co:6543/postgres?schema=public"

# Supabase API key (optional, if using Supabase API in addition to direct database access)
SUPABASE_URL="https://[YOUR-PROJECT-REF].supabase.co"
SUPABASE_ANON_KEY="your-anon-key"
SUPABASE_SERVICE_KEY="your-service-key"

# Queue Settings
QUEUE_COUNT=3
QUEUE_CONCURRENCY=2
QUEUE_TIMEOUT_MS=60000

# Persistence Settings
PERSISTENCE_ENABLED=true
PERSISTENCE_BATCH_SIZE=100 